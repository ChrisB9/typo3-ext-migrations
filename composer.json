{
  "name": "andreaswolf/migrations",
  "description": "Doctrine migrations integration for TYPO3 CMS",
  "type": "typo3-cms-extension",
  "license": "GPLv2+",
  "authors": [
    {
      "name": "Kay Strobach",
      "email": "info@kay-strobach.de"
    },
    {
      "name": "Andreas Wolf",
      "email": "dev@a-w.io"
    },
    {
      "name": "Christian Rodriguez Benthake",
      "email": "git@cben.co"
    }
  ],
  "require": {
    "doctrine/migrations": "^2.0",
    "typo3/cms-core": "^10.4 || ^11.5"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "AndreasWolf\\Migrations\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "migrations"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyze --level 8 -c phpstan-baseline.neon Classes"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "typo3/cms-composer-installers": false,
      "typo3/class-alias-loader": true
    }
  }
}
